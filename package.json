{
  "name": "testdino-mcp",
  "version": "1.0.4",
  "mcpName": "io.github.testdino-inc/testdino-mcp",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "testdino-mcp": "dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/testdino-inc/testdino-mcp"
  },
  "description": "A MCP server for TestDino.",
  "homepage": "https://docs.testdino.com/testdino-mcp/overview/",
  "scripts": {
    "build": "tsc",
    "start": "tsx dist/index.js",
    "dev": "tsx src/index.ts",
    "prepublishOnly": "npm run build",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\""
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "cursor",
    "ai"
  ],
  "author": "TestDino",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.1",
    "testdino-mcp": "file:testdino-mcp-1.0.4.tgz"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.4.6",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md",
    "docs"
  ]
}
